# ===============================
# Eko Service Specific Settings
# ===============================
# Internal port the Eko service application listens on
EKO_SERVICE_PORT=3001

# LLM Configuration for Eko Service
EKO_LLM_PROVIDER="claude" # Or "openai", "ollama", etc.
EKO_MODEL_NAME="claude-3-5-sonnet-20240620" # Ensure this is a valid model for the provider
# ANTHROPIC_API_KEY is expected to be in the root .env if EKO_LLM_PROVIDER="claude"
# OPENAI_API_KEY (for OpenAI provider) or other provider-specific keys should also be in root .env
